{
  "@context": {
    "airports": "http://williamrobertfunk.com/ontologies/airports#",
    "blade": "http://modusoperandi.com/ont/blade#",
    "dc": "http://purl.org/dc/elements/1.1/",
    "owl": "http://www.w3.org/2002/07/owl#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "wfact": "http://williamrobertfunk.com/ontologies/world-factbook#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@graph": [
    {
      "@id": "airports:wikiURI",
      "@type": "owl:DatatypeProperty",
      "rdfs:comment": "URL location on Wikipedia where more information can be located.",
      "rdfs:domain": [
        {
          "@id": "airports:Airline"
        },
        {
          "@id": "airports:Airport"
        }
      ],
      "rdfs:label": "Wikipedia URL",
      "rdfs:range": {
        "@id": "xsd:literal"
      }
    },
    {
      "@id": "airports:Airport",
      "@type": "owl:Class",
      "blade:navClass": true,
      "rdfs:comment": "\n      a complex of runways and buildings for the takeoff, landing, and maintenance of civil aircraft, with facilities for passengers.\n    ",
      "rdfs:label": "Airport"
    },
    {
      "@id": "http://williamrobertfunk.com/ontologies/airports",
      "@type": "owl:Ontology",
      "dc:contributor": "William R.A.D. Funk",
      "dc:creator": "William R.A.D. Funk",
      "dc:rights": "Copyright 2019 William R.A.D. Funk. All rights reserved.",
      "owl:versionInfo": {
        "@type": "xsd:decimal",
        "@value": "1.0"
      },
      "rdfs:comment": "\n      Identifies application specific classes and annotations that are relevant\n      only to my associated applications.\n    ",
      "rdfs:label": "Funk Ontology"
    },
    {
      "@id": "airports:name",
      "@type": [
        "owl:FunctionalProperty",
        "owl:DatatypeProperty"
      ],
      "rdfs:comment": "\n      Name or title associated with thing or place it represents.\n    ",
      "rdfs:domain": [
        {
          "@id": "airports:Airport"
        },
        {
          "@id": "airports:Airline"
        }
      ],
      "rdfs:label": "Name",
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "wfact:hasAirport",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": {
        "@id": "wfact:Country"
      },
      "rdfs:range": {
        "@id": "airports:Airport"
      }
    },
    {
      "@id": "airports:iataCode",
      "@type": [
        "owl:DatatypeProperty",
        "owl:FunctionalProperty"
      ],
      "rdfs:comment": "\n      The International Air Transportation Association code associated with an airport or airline.\n    ",
      "rdfs:domain": [
        {
          "@id": "airports:Airport"
        },
        {
          "@id": "airports:Airline"
        }
      ],
      "rdfs:label": "IATA Code",
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "wfact:hasCountry",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": {
        "@id": "airports:Airport"
      },
      "rdfs:range": {
        "@id": "wfact:Country"
      }
    },
    {
      "@id": "airports:type",
      "@type": [
        "owl:FunctionalProperty",
        "owl:DatatypeProperty"
      ],
      "rdfs:comment": "\n      The general use type of the airport (Military, Airport, Heliport, Spaceport, etc.).\n    ",
      "rdfs:domain": {
        "@id": "airports:Airport"
      },
      "rdfs:label": "Type",
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "airports:icaoCode",
      "@type": [
        "owl:DatatypeProperty",
        "owl:FunctionalProperty"
      ],
      "rdfs:comment": "\n      The International Civil Aviation Organization code associated with an airport.\n    ",
      "rdfs:domain": {
        "@id": "airports:Airport"
      },
      "rdfs:label": "ICAO Code",
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "airports:relativeSize",
      "@type": [
        "owl:FunctionalProperty",
        "owl:DatatypeProperty"
      ],
      "rdfs:comment": "\n      The relative size of the airport (small, medium, or major).\n    ",
      "rdfs:domain": {
        "@id": "airports:Airport"
      },
      "rdfs:label": "Relative Size",
      "rdfs:range": {
        "@id": "xsd:string"
      }
    },
    {
      "@id": "wfact:hasLocation",
      "@type": "owl:ObjectProperty",
      "rdfs:domain": {
        "@id": "airports:Airport"
      },
      "rdfs:range": {
        "@id": "wfact:Location"
      }
    },
    {
      "@id": "airports:Airline",
      "@type": "owl:Class",
      "blade:navClass": true,
      "rdfs:comment": "\n      an organization providing a regular public service of air transportation on one or more routes.\n    ",
      "rdfs:label": "Airline"
    }
  ]
}